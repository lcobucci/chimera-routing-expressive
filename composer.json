{
    "name": "lcobucci/chimera-routing-expressive",
    "description": "Zend Expressive implementation for lcobucci/chimera-routing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "lcobucci/chimera-foundation": "^1.0@rc",
        "lcobucci/chimera-routing": "^1.0@rc",
        "zendframework/zend-diactoros": "^1.7",
        "zendframework/zend-expressive": "^3.0",
        "zendframework/zend-expressive-fastroute": "^3.0",
        "zendframework/zend-expressive-helpers": "^5.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^4.0",
        "phpstan/phpstan": "^0.10@dev",
        "phpstan/phpstan-phpunit": "^0.10@dev",
        "phpstan/phpstan-strict-rules": "^0.10@dev",
        "phpunit/phpunit": "^7.1",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\Chimera\\Routing\\Expressive\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\Chimera\\Routing\\Expressive\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
